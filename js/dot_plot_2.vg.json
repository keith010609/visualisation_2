{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 400,
  "height": 500,
  "layer": [
    {
      "data": {"url": "https://raw.githubusercontent.com/keith010609/visualisation_2/main/data/Median%20age.csv" },
      "transform": [
        {
          "window": [{
            "op": "rank",
            "as": "rank"
          }],
          "sort": [{ "field": "Country", "order": "ascending" }]
        }, {
          "filter": "datum.rank >31 && datum.rank <= 62"
        }
      ],
      "mark": { "type": "point" , "color":"red"},
      "encoding": {
          "x": { "field": "Median age", "type": "quantitative" },
          "y": { "field": "Country" , "type": "ordinal"},
          "tooltip":[
            {"field": "Country", "type": "nominal", "title": "Country"},
            {"field": "Median age", "type": "quantitative", "title": "Median age"}
          ]
      }
    },{
      "data": { "url": "https://raw.githubusercontent.com/keith010609/visualisation_2/main/data/Life%20expectancy.csv" },
      "transform": [
          {
            "window": [{
              "op": "rank",
              "as": "rank"
            }],
            "sort": [{ "field": "Country", "order": "ascending" }]
          }, {
            "filter": "datum.rank >31 && datum.rank <= 62"
          }
        ],
      "mark": { "type": "point", "color":"blue" },
      "encoding": {
          "x": { "field": "Life expectancy", "type": "quantitative" },
          "y": { "field": "Country" , "type": "ordinal"},
          "tooltip":[
            {"field": "Country", "type": "nominal", "title": "Country"},
            {"field": "Life expectancy", "type": "quantitative", "title": "Life expectancy"}
          ]
      }
    }
  ]
  
}