{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "config": {"view": {"stroke": ""}},
    "width": 500,
    "height": 300,
    "data": {"url": "https://raw.githubusercontent.com/keith010609/visualisation_2/main/data/Sex-ratio.csv"},
    "transform": [
      {
        "window": [{
          "op": "rank",
          "as": "rank"
        }],
        "sort": [{ "field": "Sex-ratio", "order": "ascending" }]
      }, {
        "filter": "datum.rank >133 && datum.rank<=165"
      }
    ],
    "mark": "bar",
    "encoding": {
      "color": {
        "field": "Sex-ratio",
        "title": "Sex-ratio",
        "type": "quantitative",
        "scale": {
          "type": "threshold",
          "domain": [1],
          "range": ["pink", "blue"]
          }
          
      },
      "x": {"field": "Country", "type": "nominal", "title": "Country","sort":{"field":"Sex-ratio"}},
      "y": {"field": "Sex-ratio","type": "quantitative", "title": "Sex-ratio", "scale": {"domain": [0, 1.2]}},
      "tooltip":[
        {"field": "Country", "type": "nominal", "title": "Country"},
        {"field": "Sex-ratio", "type": "quantitative", "title": "Sex-ratio"}
      ],
      "y2": {"datum": 1}
    }
}
